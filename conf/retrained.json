{
    "begin_year": 2017,
    "end_year": 2021,
    "dropout": 0.0,
    "lr": 0.03,
    "batch_size": 128,
    "epoch": 500,
    "gpuid": 1,
    "loss": "mse",
    "activation": "relu",
    "scheduler": "epo",
    "train": 1,
    "y_len": 12,
    "x_len": 12,
    "data_process": 0,
    "auto_test": 1,
    "raw_data_path": "data/BAST_Data/",
    "save_data_path": "data/BAST_FastData/",
    "graph_path": "data/BAST_Graph/",
    "model_path": "res/BAST_retrain",
    "gcn":{
        "in_channel": 24,
        "out_channel": 24,
        "hidden_channel": 64
    },

    "tcn":{
        "in_channel": 1,
        "out_channel": 1,
        "kernel_size": 3,
        "dilation": 1
    },
    

    "logname": "retrained",
    "init": true,

    "strategy": "retrain",

    "detect": false,
    "detect_strategy": "original",

    "ewc": false,
    "ewc_strategy": "ewc", 
    "ewc_lambda": 1.0,
    
    "replay": false,
    "replay_strategy": "random",
    "repaly_num_samples": 100
}
