{
    "begin_year": 2021,
    "end_year": 2021,
    "dropout": 0.1,
    "lr": 0.02,
    "batch_size": 8192,
    "epoch": 500,
    "gpuid": 0,
    "loss": "huber",
    "activation": "relu",
    "scheduler": "epo",
    "train": 1,
    "y_len": 12,
    "x_len": 12,
    "data_process": 1,
    "auto_test": 1,
    "raw_data_path": "data/BAST_Data/",
    "save_data_path": "data/BAST_FastData/",
    "graph_path": "data/BAST_Graph/",
    "model_path": "res/",
    "gcn":{
        "in_channel": 12,
        "out_channel": 12,
        "hidden_channel": 256
    },

    "tcn":{
        "in_channel": 1,
        "out_channel": 1,
        "kernel_size": 3,
        "dilation": 1
    },


    "logname": "expansible",
    

    "strategy": "incremental",
    "init": true,
    "increase": true,
    "num_hops":2,
    
    "detect": false,
    "detect_strategy": "original",

    "ewc": false,
    "ewc_strategy": "ewc", 
    "ewc_lambda": 1.0,
    
    "replay": false,
    "replay_strategy": "random",
    "repaly_num_samples": 100
}
